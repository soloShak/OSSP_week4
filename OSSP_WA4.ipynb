        "#2020315232 Shakhzod\n",
        "def rotateString(s, goal):\n",
        "  # Write a code here\n",
        "  resultFlag = False\n",
        "  for rotation in range(len(s)):\n",
        "    resultString = (s*3)[len(s) + rotation : 2*len(s) + rotation]\n",
        "    if (resultString == goal):\n",
        "      resultFlag = True\n",
        "      break\n",
        "  print(resultFlag)"
      ]
    }
